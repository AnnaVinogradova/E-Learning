{% extends 'base.html.twig' %}

{% block body %}
    <div class="row progress-header">
        <div class="col-md-4 col-md-offset-3">
            <h1>You complete your test with result {{ points }}%</h1>
        </div>
    </div>
    <div class="row">
        <div class="col-md-9 col-md-offset-3">
            <div class="progress col-md-4 col-md-offset-1">
                <div class="progress-bar" role="progressbar" aria-valuenow="{{ points }}"
                     aria-valuemin="0" aria-valuemax="100" style="width:{{ points }}%">
                    <span class="sr-only">{{ points }}% Complete</span>
                </div>
            </div>
        </div>
    </div>
{% endblock %}
